{
  "expo": {
    "name": "bolt-expo-nativewind",
    "slug": "bolt-expo-nativewind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./ketomate-icons/android/playstore-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "linking": {
      "enabled": true,
      "prefixes": [
        "myapp"
      ],
      "config": {
        "screens": {
          "(tabs)": {
             "path": "/",
             "screens": {
               "index": "",
               "track": "track",
               "history": "history",
               "assistant": "assistant",
               "profile": "profile"
             }
          },
          "(auth)/login": "login",
          "+not-found": "*"
        }
      }
    },
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "53317242-4272-4c6d-b0cc-0be5fc5ec3ee"
      }
    },
    "android": {
      "package": "com.deanfieldz.boltexponativewind"
    }
  }
}
