{
  "expo": {
    "name": "KetoMate",
    "slug": "ketomate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./ketomate-icons/android/playstore-icon.png",
    "scheme": "ketomate",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "linking": {
      "enabled": true,
      "prefixes": [
        "ketomate"
      ],
      "config": {
        "screens": {
          "(tabs)": {
            "path": "/",
            "screens": {
              "index": "",
              "track": "track",
              "history": "history",
              "assistant": "assistant",
              "profile": "profile"
            }
          },
          "(auth)/login": "login",
          "+not-found": "*"
        }
      }
    },
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0f746349-c8ac-427c-a8b0-fb90438f8d99"
      }
    },
    "android": {
      "package": "com.deanfieldz.ketomate",
      "adaptiveIcon": {
        "foregroundImage": "./ketomate-icons/android/playstore-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "icon": "./ketomate-icons/android/playstore-icon.png"
    },
    "owner": "deanfieldz"
  }
}
